<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" minWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StartController">
   <children>
      <ImageView fitHeight="562.0" fitWidth="531.0" layoutY="-40.0" pickOnBounds="true">
         <image>
            <Image url="@LoginGame.jpg" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="142.0" layoutY="150.0" prefHeight="351.0" prefWidth="247.0" spacing="30.0">
         <children>
            <Button fx:id="startBtn" mnemonicParsing="false" onAction="#startGame" prefHeight="51.0" prefWidth="86.0" style="-fx-background-radius: 200;" text="Classic">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
            </Button>
            <HBox prefHeight="100.0" prefWidth="200.0" />
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="58.0" style="-fx-background-radius: 200;" text="Exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
